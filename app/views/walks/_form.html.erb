<table class="form">

<%= form_for @walk do |f| %>

<tr>
  <td><%= f.label :window_start %></td>
  <td><%= f.datetime_field :window_start %></td>
</tr>

<tr>
  <td><%= f.label :window_end %></td>
  <td><%= f.datetime_field :window_end %></td>
</tr>

<tr>
  <td><%= f.label :pet_id %></td>
  <td><%= f.text_field :pet_id %></td>
</tr>

<tr>
  <td><%= f.label :notes %></td>
  <td><%= f.text_area :notes %></td>
</tr>

<tr>
  <td><%= f.label :walk_type %></td>
  <td><%= f.text_field :walk_type %></td>
</tr>

<tr>
  <td></td><td><%= f.submit %></td>
</tr>

<% end %>

</table>
